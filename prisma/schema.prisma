generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Word_Couple {
  id             Int         @id @default(autoincrement())
  name_word_first  String
  name_word_second String
}

model Dictionary {
  id             Int         @id @default(autoincrement())
  name  String @unique
}